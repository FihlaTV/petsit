extends layout

block content

  each request in passinData
    a(href=`requests/${request.id}`) View Request
    p: a(href=`users/${request.poster_id}/`)= request.username
    p= request.pet_name
    p= request.start_date
    p= request.end_date
    p= request.notes

  a(href="/requests/add") Add New Request
